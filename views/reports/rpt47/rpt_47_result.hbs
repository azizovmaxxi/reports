                        <th class="th">Всего</th>
                        <th class="th">в т.ч. до 12-недель</th>
                        <th class="th">Всего</th>
                        <th class="th">в т.ч. родами в срок</th>
                        <th class="th">преждевременными(22-36 недель)</th>
                        <th class="th">из них очень ранними (22-27 недель)</th>
                        <th class="th">абортами</th>
                        </tr>
                        <tr class="tr" style="background: lightgray">
                        <th class="th">А</th>
                        <th class="th">Б</th>
                        <th class="th">1</th>
                        <th class="th">2</th>
                        <th class="th">3</th>
                        <th class="th">4</th>
                        <th class="th">5</th>
                        <th class="th">6</th>
                        <th class="th">7</th>
                        <th class="th">7.1</th>
                        <th class="th">8</th>
                        <th class="th">9</th>
                        <th class="th">10</th>
                        </tr>
                    </thead>
