                            <td class="td">муж</td>
                            <td class="td">жен</td>
                            <td class="td">муж</td>
                            <td class="td">жен</td>
                            <td class="td">муж</td>
                            <td class="td">жен</td>
                            <td class="td">муж</td>
                            <td class="td">жен</td>
                            <td class="td">муж</td>
                            <td class="td">жен</td>
                        </tr>
                        <tr class="tr">
                            <td class="td">{{this.c1}}</td>
                            <td class="td">{{this.c2}}</td>
                            <td class="td">{{this.c3}}</td>
                            <td class="td">{{this.c4}}</td>
                            <td class="td">{{this.c5}}</td>
                            <td class="td">{{this.c6}}</td>
                            <td class="td">{{this.c7}}</td>
                            <td class="td">{{this.c8_percent}}%</td>
                            <td class="td">{{this.c9_percent}}%</td>
                            <td class="td">{{this.c10_percent}}%</td>
                            <td class="td">{{this.c11_percent}}%</td>
                            <td class="td">{{this.c12_percent}}%</td>
                            <td class="td">{{this.c13_percent}}%</td>
                            <td class="td">{{this.c14_percent}}%</td>
                            <td class="td">{{this.c15_percent}}%</td>
                            <td class="td">{{this.c16_percent}}%</td>
                            <td class="td">{{this.c17_percent}}%</td>
                            <td class="td">{{this.c18_percent}}%</td>
                            <td class="td">{{this.c19_percent}}%</td>
                        </tr>
                    {{/each}}
                    </tbody>
                </table>
            {{/if}}
        </div>

    </div>
    <!-- END BLOCK FOR PRINT  -->

    <div id="printBtnId">
        <button class="btn btn-primary print-btn" target-print="printReport" id="printBtnId">
            <span class="glyphicon glyphicon-print" aria-hidden="true"></span> Печать
